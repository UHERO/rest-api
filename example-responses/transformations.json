{
    "transformations": [
        {
            "id": "lvl",
            "description": "Value in levels",
            "formula": "x(t)"
        },
        {
            "id": "chg",
            "description": "Change since last value",
            "formula": "x(t) - x(t-1)"
        },
        {
            "id": "ch1",
            "description": "Change since last year",
            "formula": "x(t) - x(t-n), where n is the number of observations in a year"
        },
        {
            "id": "pch",
            "description": "Percent change since last value",
            "formula": "(x(t)/x(t-1) - 1) * 100"
        },
        {
            "id": "pc1",
            "description": "Percent change since last value",
            "formula": "(x(t)/x(t-n) - 1) * 100, where n is the number of observations in a year"
        },
        {
            "id": "ytd",
            "description": "Year to date value",
            "formula": "Depends on the nature of aggregations for the given series."
        }
    ]
}
